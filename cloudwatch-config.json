{
  "agent": {
    "region": "us-east-1"
  },
  "metrics": {
    "namespace": "WebAppMetrics",
    "metrics_collected": {
      "statsd": {
        "service_address": ":8125",
        "metrics_aggregation_interval": 60
      }
    }
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/my-application.log",
            "log_group_name": "/my-app/logs",
            "log_stream_name": "static-app-log-stream",
            "timestamp_format": "%Y-%m-%d %H:%M:%S"
          }
        ]
      }
    }
  }
}
